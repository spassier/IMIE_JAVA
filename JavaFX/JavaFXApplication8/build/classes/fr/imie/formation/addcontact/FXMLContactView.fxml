<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="210.0" prefWidth="332.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.imie.formation.addcontact.FXMLContactViewController">
   <children>
      <GridPane prefHeight="400.0" prefWidth="600.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Name" textAlignment="JUSTIFY" />
            <Label text="Surname" GridPane.rowIndex="1" />
            <TextField fx:id="surnameField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label text="Birthday" GridPane.rowIndex="2" />
            <DatePicker fx:id="birthdayDate" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="nameField" alignment="CENTER" GridPane.columnIndex="1" />
            <Button fx:id="saveBtn" mnemonicParsing="false" onAction="#handleOnActionSaveBtn" text="Button" GridPane.columnIndex="1" GridPane.rowIndex="3" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
